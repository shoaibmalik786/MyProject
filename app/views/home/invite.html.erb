 $(function(){
   $("#user_gender_none").removeAttr("checked")
   $(document).keyup(function(e) {
     if (e.keyCode == 27) // reset modal on esc key press
     {
       close_request_modal();
     }
   })
   //reset modal on modal hide
   $('#request_modal').on('hidden', function () {
     close_request_modal();
   })

 });
 if ($.browser.msie && $.browser.version == 10) {
   $("*").css("font-family","Arial");
 }
 $(".input_buttons_last .request_btn a").click(function() {
   $("html, body").animate({ scrollTop: 0 }, "slow");
   return false;
 });
 $(".input_buttons_last .invite_btn a").click(function() {
   $("html, body").animate({ scrollTop: 0 }, "slow");
   return false;
 });
 
</script>

