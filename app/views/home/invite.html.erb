      <div class="control-group">
        <label class="control-label invite_heading">First Name</label>
        <div class="controls">
          <%= f.text_field :first_name,  :class=>"resend_input" %><br />
          <!-- <input class="resend_input" type="text"><br /> -->
          <span class="text_underneath">TradeYa is built on trust, real names create trusting communities</span>
        </div>
      </div>
      <div class="control-group">
        <label class="control-label invite_heading">Email</label>
        <div class="controls">
          <%= f.email_field :email,  :class=>"resend_input" %><br />
          <!-- <input class="resend_input" type="email"><br /> -->
        </div>
      </div>
      <div class="control-group">
        <label class="control-label invite_heading_gender">Gender</label>
        <div class="controls control_gender">
          <label class="radio">
            <%= f.radio_button :gender,"male", :class => "notification_mode" %>
            <label>Male</label>
          </label>
          <label class="radio">
            <%= f.radio_button :gender, "female", :class => "notification_mode" %>
            <label>Female</label>
          </label>
          <label class="radio">
            <%= f.radio_button :gender, "none", :class => "notification_mode" %>
            <label>Unspecified</label>
          </label><br />
          <!-- <span class="text_underneath">People like to know what to expect, this helps.</span> -->
        </div>
      </div>
      <div class="control-group">
        <div class="controls">
          <!-- <div class="request_submit_btn"><a href="#">Request</a> </div> -->
          <div class="request_submit_btn" ><a id="requestFormSubmitImg" class="login_btn position-click" onclick="request_invite(); return false;" href="#">Request</a></div>
        </div>
      </div>
    <% end %>
  </div>
  <div id="request_thanks" style="display:none">
    <div class="viva_revolution_text">Thank You!</div>
    <div class="welcome_revolution">Thank you for your interest in TradeYa!<br />We'll send you an invite as soon it's available.</div>
