<%= javascript_include_tag "adminscripts/admin" %>
<table>
	<tr>
		<td width="75%">
			<table class="data_point_table" style="border: solid 1px; width: 90%; margin-left: auto; margin-right: auto;">
				<tr>
					<th style="width: 70px;">Date</th>
					<th>No of Offers</th>
					<th>No of Accepted Offers</th>
					<th>% of Accepted Offers vs today offer</th>
					<th>% of Accepted Offers vs total offers(<%= t_count_offrs %>)</th>
					<th>% of Accepted Offers vs total Accepted offers(<%= t_count_a_offrs %>)</th>
					<th>No of Offers till date</th>
					<th>% of Accepted Offers vs till date offers</th>
					<th>No of Accepted Offers till date</th>
					<th>% of Accepted Offers vs till date Accepted offers</th>
				</tr>
				<% total_o = 0; %>
				<% total_a_o = 0; %>
				<% offrs_count_data.each do |d| %>
					<tr>
						<td><%= d[0] %></td>
						<td><%= d[1][0] %></td>
						<td><%= d[1][1] %></td>
						<td><%= number_with_precision(d[1][2], :precision => 2) + '%' %></td>
						<td><%= number_with_precision(d[1][3], :precision => 2) + '%' %></td>
						<td><%= number_with_precision(d[1][4], :precision => 2) + '%' %></td>
						<td><%= d[1][5] %></td>
						<td><%= number_with_precision(d[1][6], :precision => 2) + '%' %></td>
						<td><%= d[1][7] %></td>
						<td><%= number_with_precision(d[1][8], :precision => 2) + '%' %></td>
						<% total_o = total_o + d[1][0] %>
						<% total_a_o = total_a_o + d[1][1] %>
					</tr>
				<% end %>
				<tr>
					<td>Total Offers</td>
					<td><%= total_o %></td>
					<td><%= total_a_o %></td>
					<td></td><td></td><td></td><td></td><td></td><td></td><td></td>
				</tr>
			</table>
		</td>
		<td>
			<%= semantic_form_for :date_range, :builder => ActiveAdmin::FormBuilder do |f|
			   f.inputs do
			     f.input :from, :as => :datepicker
			     f.input :to, :as => :datepicker
			   end
			   f.actions
			end %>
		</td>
	</tr>
</table>
