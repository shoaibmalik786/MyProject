<h1>Listing thumb_up_downs</h1>

<table>
  <tr>
    <th>User</th>
    <th>Item</th>
    <th>Person</th>
    <th>Up down</th>
    <th>Token</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @thumb_up_downs.each do |thumb_up_down| %>
  <tr>
    <td><%= thumb_up_down.user_id %></td>
    <td><%= thumb_up_down.item_id %></td>
    <td><%= thumb_up_down.person_id %></td>
    <td><%= thumb_up_down.up_down %></td>
    <td><%= thumb_up_down.token %></td>
    <td><%= link_to 'Show', thumb_up_down %></td>
    <td><%= link_to 'Edit', edit_thumb_up_down_path(thumb_up_down) %></td>
    <td><%= link_to 'Destroy', thumb_up_down, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

